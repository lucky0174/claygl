{
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.js(doc)?$",
        "exclude": ["src/dep/glmatrix.js"]
    },
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "./doc/api/",
        "recurse": true
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "showInheritedInNav": false
    }
}