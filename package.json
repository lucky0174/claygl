{
  "name": "qtek",
  "version": "0.4.3",
  "description": "WebGL graphic library",
  "keywords": [
    "graphic",
    "webgl"
  ],
  "author": {
    "name": "Yi Shen",
    "url": "https://github.com/pissang"
  },
  "watch": {
    "essl2js": {
      "patterns": [
        "src"
      ],
      "extensions": "essl"
    },
    "amd2common": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "genentry": "node build/genentry.js",
    "prepublish": "npm run genentry"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pissang/qtek.git"
  },
  "devDependencies": {
    "esprima": "^2.7.2",
    "estraverse": "^4.1.1",
    "fs-extra": "^0.26.5",
    "glob": "^7.0.0",
    "rollup": "^0.49.3",
    "rollup-plugin-commonjs": "^8.2.1"
  }
}
